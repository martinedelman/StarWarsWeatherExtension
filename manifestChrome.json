{
    "manifest_version": 3,
    "name": "Star Wars Weather",
    "version": "1.2.9",
    "description": "Open a new tab page display a star wars background based on the weather!",
    "icons": {
      "16": "/public/assets/icon16.png",
      "32": "/public/assets/icon32.png",
      "48": "/public/assets/icon48.png",
      "64": "/public/assets/icon64.png",
      "128": "/public/assets/icon128.png"
    },
    "chrome_url_overrides" : {
      "newtab": "./public/index.html"
    },
    "permissions": [
      "geolocation"
    ],
    "action": {
      "default_popup": "./public/popup.html"
    }
  }